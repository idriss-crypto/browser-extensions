{
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "tailwind:react": "postcss tailwind.css -o src/shared/ui/providers/tailwind.build.css",
    "build:tailwind": "pnpm tailwind:react",
    "build:dev": "pnpm build:tailwind && webpack --mode=development",
    "build:prod": "pnpm build:tailwind && webpack --mode=production",
    "format": "prettier --check --config prettier.config.mjs --ignore-path .prettierignore .",
    "format:fix": "prettier --write --config prettier.config.mjs --ignore-path .prettierignore .",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --max-warnings=0 --fix",
    "circular-deps": "depcruise --config .dependency-cruiser.cjs src",
    "unused-exports": "./node_modules/.bin/ts-unused-exports ./tsconfig.json --ignoreFiles=.stories",
    "architecture-graph": "depcruise src --include-only ^src --output-type archi | dot -T svg -Grankdir=TD > architecture/graph.svg",
    "architecture-graph:folders": "depcruise src --include-only ^src --output-type ddot | dot -T svg -Grankdir=TD > architecture/folders-graph.svg",
    "type-check": "tsc --noEmit",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
