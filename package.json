{
  "scripts": {
    "buildTailwindDev": "npx tailwind -i ./src/popup/standalone.css -o ./src/popup/tailwindStandalone.css",
    "buildTailwind": "npx tailwind -i ./src/popup/standalone.css -o ./src/popup/tailwindStandalone.css --minify",
    "buildTailwindForReact": "postcss tailwind.css -o src/shared/ui/providers/tailwind.build.css",
    "build": "yarn buildTailwindDev && yarn buildTailwindForReact &&  webpack --watch",
    "buildDev": "yarn buildTailwindDev && yarn buildTailwindForReact && webpack",
    "buildOnce": "yarn buildTailwind && yarn buildTailwindForReact && webpack",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --max-warnings=0 --fix",
    "circular-deps": "depcruise --config .dependency-cruiser.cjs src",
    "architecture-graph": "depcruise src --include-only ^src --output-type archi | dot -T svg -Grankdir=TD > architecture/graph.svg",
    "architecture-graph:folders": "depcruise src --include-only ^src --output-type ddot | dot -T svg -Grankdir=TD > architecture/folders-graph.svg",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
